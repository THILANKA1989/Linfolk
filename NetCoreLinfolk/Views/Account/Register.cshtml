@model RegisterViewModel
@{
    ViewBag.Title = "Sign Up";
}
<h2 class="text-center">Contact</h2>
@section scripts{
    <script src="~/scripts/jquery.validate.min.js"></script>
    <script src="~/scripts/jquery.validate.unobtrusive.min.js"></script>
}

    <div class="row">

        <form method="post">
            <div class="col-md-6">
                <div asp-validation-summary="ModelOnly"></div>
                <div class="form-group">
                    <label asp-for="FirstName">First Name</label>
                    <input asp-for="FirstName" class="form-control" />
                    <span asp-validation-for="FirstName" class="rext-warning"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Email">Email</label>
                    <input asp-for="Email" class="form-control" />
                    <span asp-validation-for="Email" class="rext-warning"></span>
                </div>
                <div class="form-group">
                    <label asp-for="CityId">CityId</label>
                    <input asp-for="CityId" class="form-control" />
                    <span asp-validation-for="CityId" class="rext-warning"></span>
                <div class="form-group">
                    <label asp-for="Password">Password</label>
                    <input asp-for="Password" class="form-control" />
                    <span asp-validation-for="Password" class="rext-warning"></span>
                </div>
            </div>
            <div class="col-md-6">
                <div asp-validation-summary="ModelOnly"></div>
                <div class="form-group">
                    <label asp-for="LastName">Last Name</label>
                    <input asp-for="LastName" class="form-control" />
                    <span asp-validation-for="LastName" class="rext-warning"></span>
                </div>
                <div class="form-group">
                    <label asp-for="PhoneNumber">Phone Number</label>
                    <input asp-for="PhoneNumber" class="form-control" />
                    <span asp-validation-for="PhoneNumber" class="rext-warning"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Street">Street</label>
                    <input asp-for="Street" class="form-control" />
                    <span asp-validation-for="Street" class="rext-warning"></span>
                </div>
                <div class="form-group">
                    <label asp-for="ConfirmPassword">Confirm Password</label>
                    <input asp-for="ConfirmPassword" class="form-control" />
                    <span asp-validation-for="ConfirmPassword" class="rext-warning"></span>
                </div>
            </div>
            <div class="col-md-12">
                <div class="form-group">
                    <input type="submit" class="btn btn-success" value="Register" />
                </div>
            </div>
        </form>
    </div>